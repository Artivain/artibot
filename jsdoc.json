{
	"plugins": [
		"plugins/markdown"
	],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"exclude": [
			"ab-modules",
			"node-modules"
		],
		"include": [
			"index.js",
			"core",
			"logger.js",
			"interactionManager.js",
			"messages",
			"events",
			"README.md",
			"LICENSE"
		]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"template": "jsdoc-template",
		"encoding": "utf8",
		"destination": "documentation",
		"recurse": false
	}
}