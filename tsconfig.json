{
	"exclude": [
		"testing.js",
		"testing.ts",
		"dist/",
		".github/",
		"private.js",
		"docs"
	],
	"compilerOptions": {
		"module": "ES2022",
		"esModuleInterop": true,
		"target": "ES2022",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"declaration": true,
		"emitDeclarationOnly": false,
		"allowJs": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strictPropertyInitialization": true
	},
	"lib": [
		"es2015"
	],
	"ts-node": {
		"esm": true
	}
}